<schemalist gettext-domain="gnome-shell-extensions">
    <schema id="org.gnome.shell.extensions.bigsur-statusarea" path="/org/gnome/shell/extensions/bigsur-statusarea/">
        <key name="separate-date-and-notification" type="b">
            <default>true</default>
            <summary>Separate date and notification</summary>
        </key>
        <key name="activate-spacing" type="b">
            <default>false</default>
            <summary>Activate spacing</summary>
        </key>
        <key name="user-icon" type="b">
            <default>true</default>
            <summary>Show an icon instead of name</summary>
        </key>
        <key name="date-format" type="s">
            <default>""</default>
            <summary>Date format</summary>
        </key>
        <key name="spacing" type="i">
            <default>9</default>
            <summary>Horizontal Spacing</summary>
            <range min="0" max="15"/>
        </key>
        <key name="players" type="s">
            <default>""</default>
            <summary>MPRIS2 Capable Media Players</summary>
        </key>
        <key name="items" type="s">
            <default>"Calendar;1;1;calendar|Light;1;0;light|Night Light;1;0;nightlight|Volume;1;0;volume|Network;1;0;network|Bluetooth;1;0;bluetooth|Power;1;0;power|Notification;1;0;notification|User;1;0;user"</default>
            <summary>Indicators</summary>
        </key>
    </schema>
</schemalist>
